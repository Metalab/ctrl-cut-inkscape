<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
	<name>Ctrl-Cut</name>
	<id>at.metalab.ctrl-cut</id>

	<effect needs-live-preview="false">
		<object-type>all</object-type>
		<effects-menu>
			<submenu _name="Send To" />
		</effects-menu>
	</effect>

	<script>
		<command reldir="extensions" interpreter="python">ctrl-cut.py</command>
	</script>

</inkscape-extension>
